{
  "name": "aiecom",
  "version": "1.0.0",
  "description": "AI-powered e-commerce platform with Next.js frontend and Python recommendation engine",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aiecom.git"
  },
  "keywords": [
    "ecommerce",
    "ai",
    "nextjs",
    "python",
    "flask",
    "mongodb",
    "typescript",
    "tailwindcss",
    "react",
    "analytics",
    "machine-learning"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:api\"",
    "dev:frontend": "next dev",
    "dev:api": "cd recommendation-engine && python app.py",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "tsx src/scripts/seed.ts",
    "test": "npm run test:api",
    "test:api": "cd recommendation-engine && python test_api.py",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/mongoose": "^5.11.96",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "mongoose": "^8.17.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.1.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.8.0"
  }
}
